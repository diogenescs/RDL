<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>novoLogin</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>editarCategoria</from-outcome>
			<to-view-id>/categoria_edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>editarLancamento</from-outcome>
			<to-view-id>/lancamento_edit.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>editarContato</from-outcome>
			<to-view-id>/contato_edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>editarProjeto</from-outcome>
			<to-view-id>/projeto_edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>editarFrequencia</from-outcome>
			<to-view-id>/frequencia_edit.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>editarFormaDePgto</from-outcome>
			<to-view-id>/forma_de_pgto_edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>visualizarContato</from-outcome>
			<to-view-id>/contato_visualizacao.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>visualizarLancamento</from-outcome>
			<to-view-id>/lancamento_visualizacao.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>listaCategoria</from-outcome>
			<to-view-id>/categoria_list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>listaFrequencia</from-outcome>
			<to-view-id>/frequencia_list.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>listaLancamento</from-outcome>
			<to-view-id>/lancamento_list.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>listaFormaDePgto</from-outcome>
			<to-view-id>/forma_de_pgto_list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>listaProjeto</from-outcome>
			<to-view-id>/projeto_list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>listaContato</from-outcome>
			<to-view-id>/contato_list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>novaCategoria</from-outcome>
			<to-view-id>/categoria_edit.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>novaFrequencia</from-outcome>
			<to-view-id>/frequencia_edit.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>novoLancamento</from-outcome>
			<to-view-id>/lancamento_edit.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>novoProjeto</from-outcome>
			<to-view-id>/projeto_edit.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>novaFormaDePgto</from-outcome>
			<to-view-id>/forma_de_pgto_edit.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>novoContato</from-outcome>
			<to-view-id>/contato_edit.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>